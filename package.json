{
  "name": "xiaoyu-nest-template",
  "version": "0.0.1",
  "description": "nest template | nest 基础模板",
  "author": "leewei0923",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:webpack": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env RUNNING_ENV=dev nest start --watch",
    "start:hdev": "cross-env RUNNING_ENV=dev nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/static": "^7.0.4",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.8",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "fast-json-parse": "^1.0.3",
    "fastify": "^4.27.0",
    "fastify-cookie": "^5.7.0",
    "fastify-file-interceptor": "^1.0.9",
    "fastify-multer": "^2.0.2",
    "fastify-multipart": "^5.4.0",
    "mkdirp": "^3.0.1",
    "mongoose": "^8.3.4",
    "mysql2": "^3.9.7",
    "nanoid": "^5.0.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino-multi-stream": "^6.0.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rimraf": "^5.0.5",
    "run-script-webpack-plugin": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "split2": "^4.2.0",
    "typeorm": "^0.3.20",
    "webpack": "^5.91.0",
    "webpack-node-externals": "^3.0.0",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^20.12.10",
    "@types/passport-local": "^1.0.38",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
